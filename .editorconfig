root = true

[*]

# Default charset
charset = utf-8

# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true

# Remove any whitespace characters preceding newline characters
trim_trailing_whitespace = true

# Space indentation
indent_size = 4
indent_style = space
tab_width = 4

# [*.tsx]
# indent_style = tab

